# MySQL数据库配置
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=your_mysql_password
MYSQL_DATABASE=ai_travel_planner

# 服务器配置
PORT=5000
NODE_ENV=development

# JWT配置
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d

# 阿里云百炼平台配置 (OpenAI兼容模式)
# 注意：使用官方示例中的环境变量名称
DASHSCOPE_API_KEY=your_aliyun_bailian_api_key  # 您的API Key
DASHSCOPE_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1  # 北京地域
# 新加坡地域使用: https://dashscope-intl.aliyuncs.com/compatible-mode/v1
# 注意：OpenAI兼容模式不需要APPSECRET，只需要API Key